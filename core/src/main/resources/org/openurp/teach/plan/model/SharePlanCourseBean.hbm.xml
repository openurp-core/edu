<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.plan.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="SharePlanCourseBean" entity-name="org.openurp.teach.plan.SharePlanCourse">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="date"/></id>
        <many-to-one name="group" not-null="true" class="org.openurp.teach.plan.ShareCourseGroup"/>
        <many-to-one name="course" not-null="true"/>
        <property name="terms" length="20" not-null="true"/>
        <property name="compulsory" not-null="true"/>
        <many-to-one name="department"/>
        <many-to-one name="examMode"/>
        <property name="remark" length="200"/>
    </class>
</hibernate-mapping>