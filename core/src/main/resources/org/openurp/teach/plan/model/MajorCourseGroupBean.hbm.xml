<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.plan.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="MajorCourseGroupBean" entity-name="org.openurp.teach.plan.MajorCourseGroup">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="date"/></id>
        <many-to-one name="plan" not-null="true" class="org.openurp.teach.plan.MajorPlan"/>
        <many-to-one name="parent" class="org.openurp.teach.plan.MajorCourseGroup"/>
        <bag name="children" cascade="all,delete-orphan">
          <key column="parent_id"/>
          <one-to-many class="org.openurp.teach.plan.MajorCourseGroup"/>
        </bag>
        <bag name="planCourses" cascade="all,delete-orphan">
          <key column="group_id"/>
          <one-to-many class="org.openurp.teach.plan.MajorPlanCourse"/>
        </bag>
        <many-to-one name="courseType" not-null="true"/>
        <property name="credits" not-null="true"/>
        <property name="courseNum" not-null="true"/>
        <property name="groupNum" not-null="true"/>
        <property name="termCredits" length="40" not-null="true"/>
        <property name="indexno" length="20" not-null="true"/>
        <property name="remark" length="200"/>
        
        <property name="alias" length="100"/>
    </class>
</hibernate-mapping>