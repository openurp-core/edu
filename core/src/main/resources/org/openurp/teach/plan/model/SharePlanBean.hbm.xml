<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.plan.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="SharePlanBean" entity-name="org.openurp.teach.plan.SharePlan">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="date"/></id>
        <many-to-one name="project" not-null="true"/>
        <many-to-one name="education" not-null="true"/>
        <property name="name" not-null="true"/>
        <property name="grades" not-null="true"/>
        <property name="credits" not-null="true"/>
        <property name="startTerm" not-null="true"/>
        <property name="endTerm" not-null="true"/>
        <bag name="groups" cascade="all,delete-orphan">
          <key column="plan_id"/>
          <one-to-many class="org.openurp.teach.plan.ShareCourseGroup"/>
        </bag>
        <property name="remark" length="200"/>
    </class>
</hibernate-mapping>