<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.lesson.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="CourseTakeBean" entity-name="org.openurp.teach.lesson.CourseTake">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="long"><generator class="date"/></id>
        <many-to-one name="lesson"  not-null="true"/>
        <many-to-one name="course" not-null="true"/>
        <many-to-one name="semester" not-null="true"/>
        <many-to-one name="std"  not-null="true"/>
        <many-to-one name="courseTakeType" not-null="true"/>       
        <many-to-one name="electionMode" not-null="true"/> 
        <property name="turn"/>
        <property name="remark" />
        <property name="paid" not-null="true"/>
        <property name="attend" not-null="true"/>
        <many-to-one name="limitGroup"/>
        <property name="state" not-null="true"/>
        <property name="virtualCost" />
    </class>
       
</hibernate-mapping>