<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.core.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="ProjectClassroomBean" entity-name="org.openurp.teach.core.ProjectClassroom">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="date"/></id>
        <many-to-one name="room"/>
        <set name="departs" collection-type="set">
            <key column="project_classroom_id" />
            <many-to-many column="depart_id" class="org.openurp.base.Department"/>
        </set>
        <!--<property name="reserved" not-null="true"/>-->
        <many-to-one name="project" not-null="true"/>
     </class>
       
</hibernate-mapping>