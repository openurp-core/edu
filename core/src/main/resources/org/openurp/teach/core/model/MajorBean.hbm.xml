<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.core.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="MajorBean" entity-name="org.openurp.teach.core.Major">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="auto_increment"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <many-to-one name="project"/>
        <set name="directions" collection-type="set" inverse="true">
            <key column="major_id" />
            <one-to-many class="org.openurp.teach.core.Direction"/>
        </set>
       <many-to-one name="category"/>
       <set name="educations" collection-type="set" >
            <key column="major_id" />
            <many-to-many class="org.openurp.base.code.Education"/>
        </set>
        <property name="duration"/>
        <bag name="journals" collection-type="seq">
            <key column="major_id" />
            <one-to-many class="org.openurp.teach.core.MajorJournal"/>
        </bag>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="remark" length="30"/>
        <property name="abbreviation"/>
    </class>
    
</hibernate-mapping>