<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.core.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="CourseHourBean" entity-name="org.openurp.teach.core.CourseHour">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="period"/>
        <property name="weekHour"/>
        <property name="weeks"/>
        <many-to-one name="hourType"  not-null="true"/>       
        <many-to-one name="course"  not-null="true"/>
    </class>
    
</hibernate-mapping>