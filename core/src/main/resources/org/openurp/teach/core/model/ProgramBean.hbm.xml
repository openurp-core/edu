<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.core.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="ProgramBean" entity-name="org.openurp.teach.core.Program">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="auto_increment"/></id>
        <property name="name" length="100" not-null="true"/>
        <property name="grade" length="10" not-null="true"/>
        <many-to-one name="department" not-null="true"/>
        <many-to-one name="education" not-null="true"/>
        <many-to-one name="major" not-null="true"/>
        <many-to-one name="direction"/>
        <many-to-one name="stdType" />
        <property name="duration" not-null="true"/>
        <many-to-one name="degree" />
        <many-to-one name="studyType" />
        <property name="beginOn" not-null="true"/>
        <property name="endOn" not-null="true"/>
        <property name="updatedAt"/>
        <property name="remark" length="200"/>
    </class>
</hibernate-mapping>
