<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
   
    <class name="CourseGradeBean" entity-name="org.openurp.teach.CourseGrade">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <many-to-one name="course" not-null="true" />
        <property name="lessonNo"/>
        <many-to-one name="courseTakeType"  />
        <many-to-one name="courseType"  />
        <set name="examGrades" collection-type="set">
            <key column="course_grade_id" />
            <one-to-many  class="org.openurp.teach.ExamGrade"/>
        </set>
        <property name="gp"/>
        <many-to-one name="examMode"  />
        <property name="remark" length="500"/>
        <property name="beyondSubmit" not-null="true"/>
        <property name="operator"/>
        <property name="passed"/>
        <property name="personPercent" not-null="true"/>
        <property name="published" not-null="true"/>
        <property name="score"/>
        <property name="scoreText"/>
        <many-to-one name="semester"/>
        <property name="status"/>
        <many-to-one name="std"/>
        <many-to-one name="markStyle"/>
    </class>
</hibernate-mapping>