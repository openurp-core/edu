<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
     <class name="StudentJournalBean" entity-name="org.openurp.teach.StudentJournal">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="auto_increment"/></id>
        <many-to-one name="std"/>
        <property name="grade" unique="true" not-null="true"/>
        <many-to-one name="department"/>
        <many-to-one name="major"/>
        <many-to-one name="direction"/>
        <many-to-one name="adminclass"/>
        <property name="inschool" />
        <many-to-one name="status"/>
        <property name="beginOn" />
        <property name="endOn" />
        <property name="remark" length="2000" />
     </class>
     
     
</hibernate-mapping>