<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="MajorBean" entity-name="org.openurp.teach.Major">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <many-to-one name="project"/>
        <set name="directions" collection-type="set" inverse="true">
            <key column="direction_id" />
            <one-to-many class="org.openurp.teach.Direction"/>
        </set>
       <many-to-one name="category"/>
       <set name="educations" collection-type="set" inverse="true">
            <key column="major_id" />
            <one-to-many class="org.openurp.base.code.Education"/>
        </set>
        <property name="duration"/>
        <bag name="journals" collection-type="seq">
            <key column="major_id" />
            <one-to-many class="org.openurp.teach.MajorJournal"/>
        </bag>
        <property name="effectiveAt"/>
        <property name="invalidAt"/>
        <property name="remark" length="30"/>
        <property name="abbreviation"/>
    </class>
    
       
</hibernate-mapping>