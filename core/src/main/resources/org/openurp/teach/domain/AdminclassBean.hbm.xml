<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="AdminclassBean" entity-name="org.openurp.teach.Adminclass">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <property name="abbreviation"/>
        <property name="grade"/>
        <many-to-one name="department"  not-null="true"/>       
        <many-to-one name="major"  not-null="true"/>
        <many-to-one name="direction"  not-null="true"/>
        <many-to-one name="stdType"  not-null="true"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <bag name="instructors" collection-type="seq">
            <key column="adminclass_id" />
            <many-to-many column="instructor_id" class="org.openurp.teach.Teacher"/>
        </bag>
        <bag name="tutors" collection-type="seq">
            <key column="adminclass_id" />
            <many-to-many column="tutor_id" class="org.openurp.teach.Teacher"/>
        </bag>
        <set name="students" collection-type="set" inverse="true">
            <key column="adminclass_id" />
            <one-to-many class="org.openurp.teach.Student"/>
        </set>
        <property name="planCount"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
        <property name="stdCount" />
    </class>
       
</hibernate-mapping>