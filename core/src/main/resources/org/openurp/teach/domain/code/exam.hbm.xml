<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain.code"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="ExamModeBean" entity-name="org.openurp.teach.code.ExamMode">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <property name="enName" length="100" />
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
    </class>
    <class name="ExamStatusBean" entity-name="org.openurp.teach.code.ExamStatus">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <property name="enName" length="100" />
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
        <property name="attend" not-null="true"/>
        <property name="needAttendDelay" not-null="true"/>
        <property name="inputable" not-null="true"/>
    </class>
        <class name="ExamTypeBean" entity-name="org.openurp.teach.code.ExamType">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <property name="code" length="20" unique="true" not-null="true"/>
        <property name="name" length="20" unique="true" not-null="true"/>
        <property name="enName" length="100" />
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="updatedAt"/>
        <property name="remark" length="30"/>
        <many-to-one name="gradeType"  not-null="true"/>
        <property name="delay"/>
    </class>
    
</hibernate-mapping>