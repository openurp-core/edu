<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    
    <class name="MajorJournalBean" entity-name="org.openurp.teach.MajorJournal">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="auto_increment"/></id>
 <!-- 
        <property name="code" length="30" unique="true" not-null="true"/>
        <property name="name" length="50" unique="true" not-null="true"/>
        <property name="enName" length="255" unique="true" not-null="true"/>
 -->
        <many-to-one name="major"/>
        <property name="disciplineCode" length="50" unique="true" not-null="true"/>
        <many-to-one name="category"/>
        <property name="duration"/>
        <many-to-one name="education"/>
        <many-to-one name="depart"/>
        <property name="beginOn"/>
        <property name="endOn"/>
        <property name="remark" length="255"/>
      </class>
       
</hibernate-mapping>