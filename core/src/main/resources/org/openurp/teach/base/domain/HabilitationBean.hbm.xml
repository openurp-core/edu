<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.base.domain"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
     <class name="HabilitationBean" entity-name="org.openurp.teach.base.Habilitation">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="code"/></id>
        <many-to-one name="teacher"/>
        <property name="effectiveOn"/>
        <property name="invalidOn"/>
        <property name="qualified"/>
     </class>
       
</hibernate-mapping>