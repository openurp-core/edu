<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.grade.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="ExamGradeBean" entity-name="org.openurp.teach.grade.ExamGrade">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="long"><generator class="date"/></id>
        <many-to-one name="gradeType"  not-null="true"/>
        <many-to-one name="markStyle"  not-null="true"/>
        <many-to-one name="examStatus"  not-null="true"/>
        <property name="score"/>
        <property name="scoreText"/>
        <many-to-one name="courseGrade"  not-null="true"/>
        <property name="status" not-null="true"/>
        <property name="passed" not-null="true"/>
        <property name="operator"/>
        <property name="percent"/>
    </class>
       
</hibernate-mapping>
