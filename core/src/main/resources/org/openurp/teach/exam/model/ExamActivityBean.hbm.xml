<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.exam.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
 
    <class name="ExamActivityBean" entity-name="org.openurp.teach.exam.ExamActivity">
        <cache usage="read-write"/>
        <id name="id" unsaved-value="null" type="long"><generator class="auto_increment"/></id>
        <many-to-one name="lesson"  not-null="true"/>
        <many-to-one name="semester"  not-null="true"/>
        <many-to-one name="examBatch"  not-null="true"/>
        <property name="examOn" />
        <property name="beginAt" />
        <property name="endAt" />
        <property name="remark"  length="255"/>
        <!--
        <set name="examRooms" collection-type="set" inverse="true">
            <key column="exam_activity_id" />
            <many-to-many column="exam_Room_id"  class="org.openurp.teach.exam.ExamRoom"/>
        </set>
        -->  
    </class>
       
</hibernate-mapping>