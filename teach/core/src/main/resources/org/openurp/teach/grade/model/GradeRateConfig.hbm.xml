<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
 xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.teach.grade.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
    
    <class name="GradeRateConfig">
        <cache usage="read-write" region="org.openurp.teach.grade.model"/>
        <id name="id" unsaved-value="null" type="integer"><generator class="auto_increment"/></id>
        <many-to-one name="project" not-null="true"/>
        <many-to-one name="scoreMarkStyle" not-null="true"/>
        <set name="items" collection-type="seq" cascade="all,delete-orphan">
            <key column="grade_rate_config_id" />
            <one-to-many  class="org.openurp.teach.grade.model.GradeRateItem"/>
        </set>
        <property name="passScore"/>
    </class>

</hibernate-mapping>